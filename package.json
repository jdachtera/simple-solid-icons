{
  "name": "simple-solid-icons",
  "version": "0.0.2",
  "description": "SSR-safe, simple, and auto-generated icon library for SolidJS with components from popular open-source icon sets.",
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./src/index.d.ts"
    },
    "./tabler": {
      "import": "./src/tabler/index.js",
      "types": "./src/tabler/index.d.ts"
    },
    "./heroicons": {
      "import": "./src/heroicons/index.js",
      "types": "./src/heroicons/index.d.ts"
    },
    "./lucide": {
      "import": "./src/lucide/index.js",
      "types": "./src/lucide/index.d.ts"
    },
    "./feather": {
      "import": "./src/feather/index.js",
      "types": "./src/feather/index.d.ts"
    },
    "./bootstrap": {
      "import": "./src/bootstrap/index.js",
      "types": "./src/bootstrap/index.d.ts"
    },
    "./remix": {
      "import": "./src/remix/index.js",
      "types": "./src/remix/index.d.ts"
    },
    "./boxicons": {
      "import": "./src/boxicons/index.js",
      "types": "./src/boxicons/index.d.ts"
    },
    "./simpleicons": {
      "import": "./src/simpleicons/index.js",
      "types": "./src/simpleicons/index.d.ts"
    },
    "./eva/Fill": {
      "import": "./src/eva/Fill/index.js",
      "types": "./src/eva/Fill/index.d.ts"
    },
    "./eva/Outline": {
      "import": "./src/eva/Outline/index.js",
      "types": "./src/eva/Outline/index.d.ts"
    },
    "./zondicons": {
      "import": "./src/zondicons/index.js",
      "types": "./src/zondicons/index.d.ts"
    },
    "./ionicons": {
      "import": "./src/ionicons/index.js",
      "types": "./src/ionicons/index.d.ts"
    },
    "./codicons": {
      "import": "./src/codicons/index.js",
      "types": "./src/codicons/index.d.ts"
    },
    "./eva": {
      "import": "./src/eva/index.js",
      "types": "./src/eva/index.d.ts"
    }
  },
  "files": [
    "src",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf src/* dist/*",
    "update:repos": "tsx scripts/cloneIconRepos.ts",
    "generate:icons": "tsx scripts/generateIcons.ts",
    "generate:icon-index": "tsx scripts/generateIconIndex.ts",
    "generate:all": "pnpm generate:icons && pnpm generate:icon-index",
    "update:all": "pnpm update:repos && pnpm generate:all",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "solidjs",
    "icons",
    "ssr",
    "vike",
    "tabler",
    "heroicons",
    "lucide",
    "feather",
    "bootstrap",
    "remix",
    "boxicons",
    "simpleicons",
    "eva",
    "phosphor",
    "svg",
    "auto-generated"
  ],
  "author": "Jascha Dachtera",
  "license": "MIT",
  "peerDependencies": {
    "solid-js": "^1.8.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20.19.2",
    "autoprefixer": "^10.4.21",
    "glob": "^11.0.3",
    "pnpm": "^10.12.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-solid": "^2.11.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdachtera/simple-solid-icons.git"
  }
}
