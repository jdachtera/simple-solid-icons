{
  "name": "simple-solid-icons",
  "version": "1.0.0",
  "description": "SSR-safe, simple, and auto-generated icon library for SolidJS with components from popular open-source icon sets.",
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./src/index.d.ts"
    },
    "./tabler": {
      "import": "./src/tabler/index.js",
      "types": "./src/tabler/index.d.ts"
    },
    "./heroicons": {
      "import": "./src/heroicons/index.js",
      "types": "./src/heroicons/index.d.ts"
    },
    "./lucide": {
      "import": "./src/lucide/index.js",
      "types": "./src/lucide/index.d.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rm -rf src/tabler src/heroicons src/lucide src/index.js src/index.d.ts src/generated",
    "update:repos": "tsx scripts/cloneIconRepos.ts",
    "generate:icons": "tsx scripts/generateFromTemplate.ts",
    "update:all": "pnpm update:repos && pnpm generate:icons"
  },
  "keywords": [
    "solidjs",
    "icons",
    "ssr",
    "vike",
    "tabler",
    "heroicons",
    "lucide",
    "feather",
    "bootstrap",
    "remix",
    "boxicons",
    "simpleicons",
    "eva",
    "phosphor",
    "svg",
    "auto-generated"
  ],
  "author": "Jascha Dachtera",
  "license": "MIT",
  "peerDependencies": {
    "solid-js": "^1.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "glob": "^11.0.0",
    "prettier": "^3.4.2",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdachtera/simple-solid-icons.git"
  }
}
