{
  "name": "simple-solid-icons",
  "version": "0.0.9",
  "description": "SSR-safe, simple, and auto-generated icon library for SolidJS with components from popular open-source icon sets.",
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./src/index.d.ts"
    },
    "./fontawesome/solid": {
      "import": "./src/fontawesome/solid/index.js",
      "types": "./src/fontawesome/solid/index.d.ts"
    },
    "./fontawesome/regular": {
      "import": "./src/fontawesome/regular/index.js",
      "types": "./src/fontawesome/regular/index.d.ts"
    },
    "./fontawesome/brands": {
      "import": "./src/fontawesome/brands/index.js",
      "types": "./src/fontawesome/brands/index.d.ts"
    },
    "./tabler": {
      "import": "./src/tabler/index.js",
      "types": "./src/tabler/index.d.ts"
    },
    "./heroicons": {
      "import": "./src/heroicons/index.js",
      "types": "./src/heroicons/index.d.ts"
    },
    "./lucide": {
      "import": "./src/lucide/index.js",
      "types": "./src/lucide/index.d.ts"
    },
    "./feather": {
      "import": "./src/feather/index.js",
      "types": "./src/feather/index.d.ts"
    },
    "./bootstrap": {
      "import": "./src/bootstrap/index.js",
      "types": "./src/bootstrap/index.d.ts"
    },
    "./remix": {
      "import": "./src/remix/index.js",
      "types": "./src/remix/index.d.ts"
    },
    "./boxicons": {
      "import": "./src/boxicons/index.js",
      "types": "./src/boxicons/index.d.ts"
    },
    "./simpleicons": {
      "import": "./src/simpleicons/index.js",
      "types": "./src/simpleicons/index.d.ts"
    },
    "./eva/fill": {
      "import": "./src/eva/fill/index.js",
      "types": "./src/eva/fill/index.d.ts"
    },
    "./eva/outline": {
      "import": "./src/eva/outline/index.js",
      "types": "./src/eva/outline/index.d.ts"
    },
    "./zondicons": {
      "import": "./src/zondicons/index.js",
      "types": "./src/zondicons/index.d.ts"
    },
    "./ionicons": {
      "import": "./src/ionicons/index.js",
      "types": "./src/ionicons/index.d.ts"
    },
    "./antdesign/outline": {
      "import": "./src/antdesign/outline/index.js",
      "types": "./src/antdesign/outline/index.d.ts"
    },
    "./antdesign/fill": {
      "import": "./src/antdesign/fill/index.js",
      "types": "./src/antdesign/fill/index.d.ts"
    },
    "./antdesign/twotone": {
      "import": "./src/antdesign/twotone/index.js",
      "types": "./src/antdesign/twotone/index.d.ts"
    },
    "./codicons": {
      "import": "./src/codicons/index.js",
      "types": "./src/codicons/index.d.ts"
    },
    "./fontawesome": {
      "import": "./src/fontawesome/index.js",
      "types": "./src/fontawesome/index.d.ts"
    },
    "./eva": {
      "import": "./src/eva/index.js",
      "types": "./src/eva/index.d.ts"
    },
    "./antdesign": {
      "import": "./src/antdesign/index.js",
      "types": "./src/antdesign/index.d.ts"
    }
  },
  "files": [
    "src",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf src/* dist/*",
    "update:sources": "tsx scripts/updateIconSources.ts",
    "generate:icons": "tsx scripts/generateIcons.ts",
    "generate:icon-index": "tsx scripts/generateIconIndex.ts",
    "generate:all": "pnpm generate:icons && pnpm generate:icon-index",
    "update:all": "pnpm update:sources && pnpm generate:all",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "solidjs",
    "icons",
    "ssr",
    "vike",
    "tabler",
    "heroicons",
    "lucide",
    "feather",
    "bootstrap",
    "remix",
    "boxicons",
    "simpleicons",
    "eva",
    "svg",
    "auto-generated"
  ],
  "author": "Jascha Dachtera",
  "license": "MIT",
  "peerDependencies": {
    "solid-js": "^1.8.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20.19.2",
    "autoprefixer": "^10.4.21",
    "glob": "^11.0.3",
    "pnpm": "^10.12.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-solid": "^2.11.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdachtera/simple-solid-icons.git"
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.4.2",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@heroicons/react": "^2.2.0",
    "@tabler/icons": "^3.34.0",
    "@vscode/codicons": "^0.0.36",
    "bootstrap-icons": "^1.13.1",
    "boxicons": "^2.1.4",
    "feather-icons": "^4.29.2",
    "ionicons": "^8.0.9",
    "lucide-static": "^0.525.0",
    "remixicon": "^4.6.0",
    "simple-icons": "^15.4.0",
    "tar": "^7.4.3"
  }
}
